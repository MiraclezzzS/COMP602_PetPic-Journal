<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.LoginUserDao">

	
	
	
    <resultMap type="com.entity.LoginUserEntity" id="yonghuMap">
        <result property="username" column="username"/>
        <result property="name" column="name"/>
        <result property="password" column="password"/>
        <result property="sex" column="sex"/>
        <result property="phone" column="phone"/>
        <result property="headImage" column="head_image"/>
    </resultMap>


	<select id="selectListVO"
		resultType="com.entity.vo.LoginUserVO" >
		SELECT * FROM loginuser  loginuser         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.LoginUserVO" >
		SELECT  loginuser.* FROM loginuser  loginuser
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.LoginUserView" >

		SELECT  loginuser.* FROM loginuser  loginuser 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.LoginUserView" >
		SELECT * FROM loginuser  loginuser <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

	
</mapper>
