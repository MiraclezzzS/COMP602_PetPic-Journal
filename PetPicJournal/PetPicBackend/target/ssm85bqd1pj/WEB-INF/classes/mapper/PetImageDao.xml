<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.PetImageDao">

    <resultMap type="com.entity.PetImageEntity" id="chongwuxiangceMap">
        <result property="pettitle" column="imagetitle"/>
        <result property="imagetime" column="imagetime"/>
        <result property="imageclass" column="imageclass"/>
        <result property="petimage" column="petimage"/>
        <result property="username" column="username"/>
        <result property="imageremark" column="imageremark"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.PetImageVO" >
		SELECT * FROM petimage  petimage         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.PetImageVO" >
		SELECT  petimage.* FROM petimage  petimage
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.PetImageView" >

		SELECT  petimage.* FROM petimage  petimage 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.PetImageView" >
		SELECT * FROM petimage  petimage <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

	
</mapper>
